{
    "name": "futurist.network",
    "version": "0.0.1",
    "description": "Guiding the future in a net positive direction by allowing the public to propose, fund, and work on the themes and technologies of the future.",
    "engines": {
        "node": "12.18.2",
        "yarn": "1.15.2"

    },

    "scripts": {
        "dev": "yarn --cwd packages/backend dev;yarn --cwd packages/frontend dev;",
        "build:frontend": "yarn --cwd packages/frontend build",
        "serve:backend": "yarn --cwd packages/backend serve",
        "postinstall": "yarn --cwd packages/frontend;yarn --cwd packages/backend"


    },
